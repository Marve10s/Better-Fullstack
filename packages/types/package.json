{
  "name": "@better-fullstack/types",
  "version": "1.3.2",
  "description": "TypeScript types and schemas for Better Fullstack CLI",
  "keywords": [
    "better-fullstack",
    "cli",
    "schemas",
    "types",
    "typescript"
  ],
  "homepage": "https://better-fullstack-web.vercel.app/",
  "license": "MIT",
  "author": "Marve10s",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marve10s/Better-Fullstack.git",
    "directory": "packages/types"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./schemas": {
      "types": "./dist/schemas.d.mts",
      "default": "./dist/schemas.mjs"
    },
    "./json-schema": {
      "types": "./dist/json-schema.d.mts",
      "default": "./dist/json-schema.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint . && tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "tsdown": "^0.17.2",
    "typescript": "^5.9.3"
  }
}
